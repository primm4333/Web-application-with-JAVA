<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1" >
         
         
         
     <servlet>
    	<servlet-name>LoginS</servlet-name>
    	<servlet-class>oop_train.LoginS</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>LoginS</servlet-name>
    	<url-pattern>/login</url-pattern>
    </servlet-mapping>
         
         
         
         
    <servlet>
    	<servlet-name>customerCreate</servlet-name>
    	<servlet-class>oop_train.customerCreate</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>customerCreate</servlet-name>
    	<url-pattern>/insert</url-pattern>
    </servlet-mapping>
         
         
    <servlet>
		<servlet-name>updateCustomerS</servlet-name>
		<servlet-class>oop_train.updateCustomerS</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>updateCustomerS</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>deleteCustomerS</servlet-name>
		<servlet-class>oop_train.deleteCustomerS</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>deleteCustomerS</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
          
         
         

         
</web-app>